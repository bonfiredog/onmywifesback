V<<nobr>>\u000a/%Constellation Code - Explains for all constellations*%/\u000a/%First line sets up two states; that you've completed the constellation, or that you haven't.%/\u000a<<if $meandertotal < 7>>\u000a/%This is what appears if you have not completed the constellation. i.e. A group of single stars, some pressed, some not.%/\u000a<<if $meanderpress1 is "no">>\u000a/%This code allows you to click the star, which will flash a message of encouragement, set the star so that you cannot click it again, and register the click against the constellation total%/\u000a<<replace>><div id="meander1">*</div><<becomes>><<set $meanderpress1 to "yes">><<set $meandertotal += 1>><<if $meandertotal < 7>><div id="meander1">*<<timedreplace 3s>>&nbsp;&nbsp;<<encouragement>><<becomes>><<endtimedreplace>></div>\u000a/%---------------------------- This big block basically checks if you were in the middle of any other constellation, and if so, resets that constellation. The mechanic here is designed for you to identify a constellation consecutively, and punishes you if you click something else. ----------------------%/\u000a<<if $wallistotal > 0 and $wallistotal < 8>>\u000a<<set $wallistotal = 0; $wallispress1 to "no"; $wallispress2 to "no"; $wallispress3 to "no"; $wallispress4 to "no"; $wallispress5 to "no"; $wallispress6 to "no"; $wallispress7 to "no"; $wallispress8 to "no">> \u000a<<elseif $codtotal > 0 and $codtotal < 4>>\u000a<<set $codtotal = 0; $codpress1 to "no"; $codpress2 to "no"; $codpress3 to "no"; $codpress4 to "no">>\u000a<<elseif $goodboytotal > 0 and $goodboytotal < 4>>\u000a<<set $goodboytotal = 0; $goodboypress1 to "no"; $goodboypress2 to "no"; $goodboypress3 to "no"; $goodboypress4 to "no">>\u000a<<elseif $termtotal > 0 and $termtotal < 18>>\u000a<<set $termtotal = 0; $termpress1 to "no"; $termpress2 to "no"; $termpress3 to "no"; $termpress4 to "no"; $termpress5 to "no"; $termpress6 to "no"; $termpress7 to "no"; $termpress8 to "no"; $termpress9 to "no